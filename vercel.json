{
  "builds": [{
    "src": "project3/wsgi.py",
    "use": "@vercel/python",
    "config": { "maxLambdaSize": "15mb", "runtime": "python3.11" }
  }],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "project3/wsgi.py"
    }
  ],
  "hooks": {
    "postdeploy": "python3 manage.py migrate"
  }
}